<header>
    <h1>Categories</h1>
</header>
<main>
    <% @group.each do |item|%>
        <div>
            <img src = <%= item.icon%> alt = 'icon'>
            <h2><%= link_to item.name, category_path(item.id) %></h2>
            <p><%= item.total %></p>
            <p><%= item.created_at%></p>
        </div>
    <%end%>
</main>
<%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
<%= link_to "new category", new_category_path, :method => :delete %>
